<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CardsLayout">
        <attr name="minChildWidth" format="dimension"/>
        <attr name="childHeaderHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CardsLayout_Layout">
        <attr name="state" format="enum">
            <enum name="expanded" value="0"/>
            <enum name="collapsed" value="1"/>
        </attr>
    </declare-styleable>
</resources>